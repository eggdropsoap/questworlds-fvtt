{
	"name": "questworlds",
	"title": "QuestWorlds (Unofficial)",
	"description": "A Foundry VTT system to play QuestWorlds, the rules-lite storytelling RPG from Chaosium. Based on the rules in the QuestWorlds SRD.",
	"version": "0.1.1",
	"author": "eggdropsoap",
	"license": "https://github.com/eggdropsoap/questworlds-fvtt/blob/master/LICENSE.txt",
	"readme": "https://github.com/eggdropsoap/questworlds-fvtt/blob/master/README.md",
	"changelog": "https://github.com/eggdropsoap/questworlds-fvtt/blob/master/CHANGELOG.md",
	"bugs": "https://github.com/eggdropsoap/questworlds-fvtt/issues",
	"minimumCoreVersion": "0.8.8",
	"compatibleCoreVersion": "0.8.9",
	"flags": {
		"allowBugReporter": true
	},
	"esmodules": [
		"module/qw.mjs",
		"../scripts/greensock/esm/gsap-core.js"
	],
	"dependencies": [
		{ "name": "socketlib", "type": "module" }
	],
	"styles": [ "css/qw.css" ],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "lang/en.json"
		}
	],
	"socket": true,
	"initiative": "1d20",
	"gridDistance": 10,
	"gridUnits": "ft",
	"primaryTokenAttribute": null,
	"secondaryTokenAttribute": null,
	"url": "https://github.com/eggdropsoap/questworlds-fvtt",
	"manifest": "https://github.com/eggdropsoap/questworlds-fvtt/blob/master/system.json",
	"download": ""
